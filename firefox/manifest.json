{
  "name": "Restore Old Roblox Server List",
  "manifest_version": 2,
  "version": "1.2.4",
  "description": "Restores the old roblox server list & allows searching of User ID's in games",
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    },
    "default_title": "Settings",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "https://*.roblox.com/games/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "images/*.png",
    "styles.css",
    "content.js",
    "load.js",
    "panel.html"
  ]
}